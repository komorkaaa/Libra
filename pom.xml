            <url>https://github.com/komorkaaa</url>
        </developer>
    </developers>

    <scm>
        <connection>scm:git:https://github.com/komorkaaa/MeetFlow.git</connection>
        <developerConnection>scm:git:git@github.com:komorkaaa/MeetFlow.git</developerConnection>
        <url>https://github.com/komorkaaa/MeetFlow</url>
        <tag>HEAD</tag>
